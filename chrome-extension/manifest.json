{
  "manifest_version": 3,
  "name": "Tabela Collector (Auto Poll)",
  "version": "1.3.0",
  "description": "Coleta tabela (Angular Material ou HTML) com paginação e envia JSON ao backend. Faz polling /next a cada 30s.",
  "permissions": [
    "scripting",
    "tabs",
    "activeTab",
    "alarms"
  ],
  "host_permissions": [
    "https://consultaunificadapje.tse.jus.br/#/public/inicial/index/*",
    "http://localhost:8000/*"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "action": {
    "default_title": "Tabela Collector"
  }
}